public class dizidekiElemanlarinOrtalamasi {
    public static void main(String[] args) {
        int [] array={1,2,3,4};
        double avarage;
        double sum=0;
        for(int i:array){
            sum+=i;
        }
        avarage=sum/array.length;
        System.out.println(avarage);
    }
}
